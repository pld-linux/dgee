/var/log/dgee/degee.log {
	su root http
	create create 660 root http
	olddir /var/log/archive/dgee
	postrotate
		/etc/rc.d/init.d/dgee restart
	endscript
}

/var/log/dgee/stdout {
	su root http
	create create 660 root http
	olddir /var/log/archive/dgee
	postrotate
		/etc/rc.d/init.d/dgee restart
	endscript
}

/var/log/dgee/stderr {
	su root http
	create create 660 root http
	olddir /var/log/archive/dgee
	postrotate
		/etc/rc.d/init.d/dgee restart
	endscript
}
